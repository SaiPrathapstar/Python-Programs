    "def Armstrong(num):\n",
    "    s = 0\n",
    "    temp = num\n",
    "    rem = 0\n",
    "    while num > 0:\n",
    "        rem = num%10\n",
    "        s = s + rem*rem*rem\n",
    "        num = int(num/10)\n",
    "    if s == temp:\n",
    "        print(\"Armstrong\")\n",
    "    else:\n",
    "        print(\"not a Armstrong\")"
   ]
